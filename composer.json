{
	"name": "mrhenry/gs-uploads",
	"description": "WordPress plugin to store uploads on Google Cloud Storage",
	"homepage": "https://github.com/mrhenry/gs-uploads",
	"keywords": [
		"wordpress"
	],
	"license": "GPL-2.0+",
	"support"    : {
		"issues": "https://github.com/mrhenry/gs-uploads/issues",
		"source": "https://github.com/mrhenry/gs-uploads"
	},
	"type": "wordpress-plugin",
	"require": {
		"composer/installers": "^1.0 || ^2.0",
		"google/cloud-storage": "^1.49"
	},
	"archive": {
		"exclude": [
			".git",
			".github",
			"tests",
			"composer.lock"
		]
	},
	"config": {
		"allow-plugins": {
			"composer/installers": true,
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"require-dev": {
		"wp-coding-standards/wpcs": "^3.0",
		"phpunit/phpunit": "^9"
	}
}
